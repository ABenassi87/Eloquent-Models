{
    "name": "faustbrian/eloquent-models",
    "description": "Adds basic models for Laravel",
    "keywords": ["laravel", "framework", "Eloquent-Models", "Eloquent Models", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de",
        "role": "Developer"
    }],
    "require": {
        "php": "^5.6 || ^7.0",
        "faustbrian/laravel-service-provider": "^1.0",
        "illuminate/database": "5.1.* || 5.2.* || 5.3.*",
        "nesbot/carbon": ">=1.21.0",
        "faustbrian/eloquent-schema": "^1.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "phpunit/phpunit": "^5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Eloquent\\Models\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Eloquent\\Models\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
